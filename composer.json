{
    "name": "combro2k/cms-bundle",
    "type": "symfony-bundle",
    "description": "CMS bundle for use with Symfony2",
    "keywords": ["cms", "bundle"],
    "homepage": "http://github.com/combro2k/CMSBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "M. van Maurik",
            "email": "git@vmaurik.nl"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "kriswallsmith/buzz": "dev-master"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1,<2.2-dev"
    },
    "autoload": {
        "psr-0": { "CMS\\Bundle\\": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "CMS/Bundle"
}
