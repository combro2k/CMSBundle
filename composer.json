{
    "name": "combro2k/mordred-cms-bundle",
    "type": "symfony-bundle",
    "description": "CMS Bundle for use with Symfony2",
    "keywords": ["cms", "bundle"],
    "homepage": "http://github.com/combro2k/CMSBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "M. van Maurik",
            "email": "git@vmaurik.nl"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "kriswallsmith/buzz": "dev-master",
        "sensio/framework-extra-bundle": "*",
        "dancannon/php-wowapi": "dev-master",
        "knplabs/knp-snappy-bundle": "dev-master",
	"friendsofsymfony/user-bundle": "dev-master"
    },
    "require-dev": {
        "sensio/framework-extra-bundle": "*",
        "symfony/framework-bundle": ">=2.1,<2.2-dev"
    },
    "autoload": {
        "psr-0": { "Mordred\\Bundle\\CMSBundle\\": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Mordred/Bundle/CMSBundle"
}
